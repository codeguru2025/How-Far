    "expo-secure-store": "~15.0.8",
    "expo-speech": "^14.0.8",
    "expo-splash-screen": "^31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "^15.0.10",
    "paynow": "^2.2.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-google-places-autocomplete": "^2.6.1",
