    "@react-navigation/native-stack": "^7.8.6",
    "@rnmapbox/maps": "^10.2.10",
    "@supabase/supabase-js": "^2.87.1",
    "expo": "~54.0.28",
    "expo-build-properties": "^1.0.10",
    "expo-camera": "~17.0.10",
    "expo-crypto": "^15.0.8",
    "expo-linking": "^8.0.10",
