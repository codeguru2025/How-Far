{
  "name": "how-far",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "add-mapbox-maven": "node scripts/add-mapbox-maven.js",
    "eas-build-post-install": "node scripts/add-mapbox-maven.js || true",
    "start": "expo start",
    "dev": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "validate-env": "node scripts/validate-env.js",
    "validate-env:strict": "node scripts/validate-env.js --strict",
    "prebuild": "npm run validate-env",
    "build": "node scripts/build.js",
    "build:dev": "node scripts/build.js development all",
    "build:preview": "node scripts/build.js preview all",
    "build:prod": "node scripts/build.js production all",
    "build:android": "node scripts/build.js production android",
    "build:ios": "node scripts/build.js production ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "clean": "rm -rf node_modules .expo && npm install",
    "reset": "rm -rf node_modules .expo package-lock.json && npm install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "5.0.1",
